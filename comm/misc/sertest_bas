100 DEFine PROCedure GETSER
110  OPEN#4;"SER1H"
120  REPeat lup
130   a$=INKEY$(#4,200)
140   IF a$="" THEN EXIT lup
150   PRINT a$;
160  END REPeat lup
170  CLOSE#4
180 END DEFine
190 :
200 DEFine PROCedure PUTSER
210  OPEN#4;"SER1H"
220  LIST#4
230  CLOSE#4
240 END DEFine
250 :
260 DEFine PROCedure SAV
270  DELETE win1_SERTEST_bas
280  SAVE win1_SERTEST_bas
290 END DEFine
